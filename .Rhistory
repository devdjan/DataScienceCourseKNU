my_ifelse<-function(x,exp,n){
if (exp == "<"){
return(x[x<n])
}
else if (exp==">"){
return(x[x>n])
}
else if (exp=="<="){
return(x[x<=n])
}
else if (exp==">="){
return(x[x>=n])
}
else if (exp=="=="){
return(x[x==n])
}
else x
}
